# Azure AI Search Simulator - Environment Variables
# Copy this file to .env and fill in your values.
# The .env file is gitignored and will NOT be committed.

# Simulator base URL
BASE_URL=https://localhost:7250

# API version
API_VERSION=2024-07-01

# API keys (admin has full access, query is read-only)
ADMIN_KEY=admin-key-12345
QUERY_KEY=query-key-67890

# Custom Skills API base URL (for CustomSkillSample)
SKILLS_BASE_URL=http://localhost:5260

# Storage configuration (for lego-search-setup-sim.http or real Azure sources)
STORAGE_ACCOUNT=your-storage-account
STORAGE_KEY=your-storage-key
STORAGE_CONTAINER=your-container
STORAGE_FOLDER=your-folder

# Entra ID / Azure AD bearer token (for entra-id-auth-requests.http)
# Obtain via: az account get-access-token --resource https://search.azure.com --query accessToken -o tsv
ENTRA_TOKEN=
